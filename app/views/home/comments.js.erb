$("#video_<%= @video_id %> .show_comments").css( { display : "none" } );
$("#video_<%= @video_id %> .comments_container").html("<%= j render "comments_list.html" %>");
$("#video_<%= @video_id %>").find(".res-more").css({ "display" : "block" });

if ( $("#video_<%= @video_id %>").find(".res-data").css("overflow") == "hidden" ) {
  $("#video_<%= @video_id %>").find(".res-more").click();
}
